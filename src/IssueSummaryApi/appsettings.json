{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Auth": {
    "ApiKey": "{{RANDOM_KEY}}"
  },
  "OpenApi": {
    "Title": "GitHub Issues Summary",
    "Version": "v1",
    "Server": "https://localhost:5001",
    "IncludeOnDeployment": false
  },
  "GitHub": {
    "Agent": "Issue Summary Bot"
  },
  "AOAI": {
    "DeploymentId": "{{AOAI_DEPLOYMENT_ID}}",
    "Endpoint": "{{AOAI_ENDPOINT_URL}}",
    "ApiKey": "{{AOAI_API_KEY}}"
  },
  "AllowedHosts": "*"
}